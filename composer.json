{
    "name": "sbsedv/memory-cache",
    "type": "library",
    "description": "In-Memory Psr-6 cache implementation.",
    "license": "MIT",
    "homepage": "https://github.com/SBSEDV/memory-cache-php",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "keywords": [
        "cache",
        "psr6",
        "memory cache"
    ],
    "require": {
        "php": ">=8.0",
        "psr/cache": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "provide": {
        "psr/cache-implementation": "2.0|3.0"
    },
    "autoload": {
        "psr-4": {
            "SBSEDV\\Component\\Cache\\InMemory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SBSEDV\\Component\\Cache\\InMemory\\Tests\\": "Tests/"
        }
    }
}
